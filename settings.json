{
  "env": {
    "ANTHROPIC_MODEL": "sonnet"
  },
  "permissions": {
    "allow": [
      "Read(*)",
      "WebFetch(domain:*)",
      "mcp__sequentialthinking__sequentialthinking_tools"
    ],
    "defaultMode": "default"
  },
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "uv run C:/Users/NATH/.claude/hooks/pre_tool_use.py",
            "timeout": 30
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Write|Edit|MultiEdit",
        "hooks": [
          {
            "type": "command",
            "command": "uv run C:/Users/NATH/.claude/hooks/post_tool_use.py",
            "timeout": 60
          }
        ]
      }
    ],
    "SubagentStop": [
      {
        "matcher": "Write|Edit|MultiEdit",
        "hooks": [
          {
            "type": "command",
            "command": "uv run C:/Users/NATH/.claude/hooks/subagent_stop.py",
            "timeout": 10
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": "uv run C:/Users/NATH/.claude/hooks/stop.py"
          }
        ]
      }
    ],
    "Notification": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "uv run C:/Users/NATH/.claude/hooks/notifications.py"
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "uv run C:/Users/NATH/.claude/hooks/pre_compact.py"
          }
        ]
      }
    ]
  }
}